  <row>
    <panel>
      <title>Predictions</title>
      <table>
        <title>FROM::</title>
        <search>
          <query>sourcetype=_jsonFUTURE BOMPREDdate 
 | eval Day = strftime(_time,"%a") | eval Date = strftime(_time,"%F") 
 | eventstats latest(BOMPREDapiPollTime) as latestPollTime 
 | where BOMPREDapiPollTime=latestPollTime 
 | sort _time 
 | eval APItime = strftime(BOMPREDapiPollTime,  "%H:%M %p [%d-%m-%Y]")
 | table Day, Date, BOMPREDrainChance, BOMPREDrainMM, BOMPREDtempMin, BOMPREDtempMax, BOMPREDdescBrief, BOMPREDdescDetail, APItime
 | rename BOMPREDrainChance as "Rain%", BOMPREDrainMM as "mm", BOMPREDtempMin as "Min", BOMPREDtempMax as "Max", BOMPREDdescBrief as "ForecastBrief", BOMPREDdescDetail as "ForecastDetail"</query>
          <earliest>-d@d</earliest>
          <latest>+6d</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
